  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dotneteer/watson.git"
  },
  "dependencies": {
    "import-local": "^3.0.2"
  },
  "peerDependencies": {
    "@dotneteer/watsharp-core": ">=0.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.2"
  }
